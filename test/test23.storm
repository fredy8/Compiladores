class BankAccount {
  int balance;
  function void init() {
    balance = 0;
    return;
  }
  function void deposit(int money) {
    balance = balance + money;
    return;
  }
  function bool withdraw(int money) {
    if (money <= balance) {
      balance = balance - money;
      return true;
    } else {
      return false;
    }
  }
}

