class Matrix {
  int arr[20];
  int NN, MM;
  function void init(int N, int M) {
    int i;
    NN = N;
    MM = M;
    for (i = 0; i < N*M; i = i + 1) {
      .arr[i] = 0;
    }
    return;
  }
  function int getRows() {
    return NN;
  }
  function int getCols() {
    return MM;
  }
  function int get(int i, int j) {
    return arr[i * MM + j];
  }
  function void set(int i, int j, int value) {
    .arr[i * MM + j] = value;
    return;
  }
}

